      DatabaseAlias = 'SISADMIN'
      SQL = 
        'select cli.nomefornecedor, sum(movd.PRECO) AS VALORTITULO , sum(' +
        'movd.outro_credito) as QTD_DE_PROCEDIMENTO , prod.produto as nom' +
        'e ,'#13#10'pl.nome '#13#10'from compra rec  '#13#10'inner join fornecedor cli on r' +
        'ec.codfornecedor = cli.codfornecedor  '#13#10'inner join movimentodeta' +
        'lhe movd on movd.codmovimento = rec.codmovimento  '#13#10'inner join p' +
        'rodutos prod on prod.codproduto = movd.codproduto  '#13#10'inner join ' +
        'plano pl on pl.conta = prod.conta_estoque'#13#10'where rec.DATAINCLUSA' +
        'O between '#39'09/28/13'#39' and '#39'09/28/13'#39' and rec.CODFORNECEDOR = 1'#13#10'g' +
        'roup by cli.nomefornecedor, movd.PRECO , movd.outro_credito , pr' +
        'od.produto ,pl.nome'#13#10#13#10'/*'#13#10'select              '#13#10'       SUM(rec.' +
        'VALORTITULO) AS VALORTITULO ,'#13#10'       SUM(rec.outro_credito) as ' +
        'QTD_DE_PROCEDIMENTO,       '#13#10'       pl.nome ,    '#13#10'       pl.con' +
        'tapai,'#13#10'       pla.nome'#13#10#13#10'       from PAGAMENTO rec '#13#10'       in' +
        'ner join FORNECEDOR cli on cli.CODFORNECEDOR=rec.CODFORNECEDOR'#13#10 +
        '       inner join PLANO pl on pl.codigo = rec.contacredito'#13#10'    ' +
        '  left outer join PLANO pla on pla.conta = pl.contapai'#13#10'       w' +
        'here  rec.outro_credito <> '#39'0'#39'  and (rec.DATAINCLUSAO BETWEEN  '#39 +
        '01/01/13'#39' and '#39'01/31/13'#39')'#13#10'       group by '#13#10'       pla.conta, '#13 +
        #10'       pl.contapai,'#13#10'       pl.nome,'#13#10'       pla.nome'#13#10#13#10'*/'
    end
